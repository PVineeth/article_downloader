            <ce:abstract class="author-highlights" xml:lang="en" id="ab005" view="all">
               <ce:section-title id="st065">Highlights</ce:section-title>
               <ce:abstract-sec id="as005" view="all">
                  <ce:simple-para id="sp0005" view="all">
                     <ce:list id="l0005">
                        <ce:list-item id="o0005">
                           <ce:label>•</ce:label>
                           <ce:para id="p0135" view="all">Chalcogenide glass fiber doped with Pr<ce:sup loc="post">3+</ce:sup> successfully fabricated.</ce:para>
                        </ce:list-item>
                        <ce:list-item id="o0010">
                           <ce:label>•</ce:label>
                           <ce:para id="p0140" view="all">Mid-infrared emission from Pr<ce:sup loc="post">3+</ce:sup> chalcogenide glass fiber observed.</ce:para>
                        </ce:list-item>
