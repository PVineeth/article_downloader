                  <ce:bib-reference id="b0060">
                     <ce:label>[12]</ce:label>
                     <sb:reference id="h0060">
                        <sb:contribution langtype="en">
                           <sb:authors>
                              <sb:author>
                                 <ce:given-name>Suresh Kumar</ce:given-name>
                                 <ce:surname>Sharma</ce:surname>
                              </sb:author>
                              <sb:author>
                                 <ce:given-name>R.K.</ce:given-name>
