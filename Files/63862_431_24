<script type="text/javascript">

jQuery(function($) {

	var currentPid = '2190-4286-7-24'
	var articleDoi = '10.3762/bjnano.7.24'

  if(checkSessionStorage() === undefined) {
  	$("#pager").hide();
  	$("#backBtn").hide();
    return;
  }



	// get the breadcrumbs for the current article
//	getBreadCrumbList(false, currentPid, articleDoi);
	
	var pidList = getPublicIdList();
	if(pidList == null || pidList.length == 1 || !isCurrentPublicIdContainedIdList(currentPid)){
		$("#pager").hide();
		$("#backBtn").hide();
		return;
	}

	var nextPid =nextPublicId('2190-4286-7-24');
	var prevPid =previousPublicId('2190-4286-7-24');

	if(nextPid == -1){
		$('#nextBtn').addClass("disabled").attr("disabled", "disabled");
	}
	if(prevPid == -1){
		$('#previousBtn').addClass("disabled").attr("disabled", "disabled");
	}

  // reset the breadcrumbs and article list related values
  // when a link of "similar articles" is clicked
	jQuery('.panel__micro h5 a').click(function(){
    //console.log('similar article clicked');
    //clearBreadCrumbList();
    //setShowPager(false);
    //clearPublicIdList();
	});
});


function backToArticleList(){
  if(checkSessionStorage() === undefined){
    return;
  }
  
	var journal = 'bjnano';
	var backPage = getBackPage();
	
	document.location.href = '/' + journal + '/' + backPage;
}

function gotoArticle(direction) {
	if(checkSessionStorage() === undefined){
    return;
  }

  
	var journal = 'bjnano';
	var targetPublicId;

	if(direction == 'next'){
		targetPublicId =nextPublicId('2190-4286-7-24');
	}
	if(direction == 'previous'){
		targetPublicId =previousPublicId('2190-4286-7-24');
	}

	var splitted = targetPublicId.split("-");
	var vol = splitted[2];
	var an = splitted[3];

	url = '/' + journal + '/articles/'+ vol + '/'+an;
  document.location.href = url;
}
</script>

