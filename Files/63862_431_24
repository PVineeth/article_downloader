	var clearStorage = false
	var clearBCList =null


  if(checkSessionStorage() === undefined){
    return;
  }
	    
		
	if(clearStorage){
		clearPublicIdList();
	}

	if(clearBCList){
		clearBreadCrumbList();
	}
});
