            </ce:dochead>
            <ce:title id="tit0005">Applicability of Augis–Bennett relation for determination of activation energy of glass transition in some Se rich chalcogenide glasses</ce:title>
            <ce:author-group id="aug0005">
               <ce:author id="aut0005">
                  <ce:given-name>Shipra</ce:given-name>
                  <ce:surname>Saraswat</ce:surname>
                  <ce:cross-ref id="crf0005" refid="aff0005">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
                  <ce:cross-ref id="crf0010" refid="aff0010">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
                  <ce:cross-ref id="crf0015" refid="cor0005">
                     <ce:sup loc="post">⁎</ce:sup>
                  </ce:cross-ref>
                  <ce:e-address id="eadd0005" type="email">dr_shipra_saraswat@yahoo.com</ce:e-address>
               </ce:author>
               <ce:author id="aut0010">
                  <ce:given-name>Neeraj</ce:given-name>
                  <ce:surname>Mehta</ce:surname>
                  <ce:cross-ref id="crf0020" refid="aff0005">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
                  <ce:cross-ref id="crf0025" refid="aff0010">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author id="aut0015">
