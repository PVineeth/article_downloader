                                             <mml:mn>2</mml:mn>
                                             <mml:mi mathvariant="normal">r</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mo linebreak="goodbreak">−</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>N</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>τ</mml:mi>
                                          <mml:mrow>
                                             <mml:mn>21</mml:mn>
                                             <mml:mi mathvariant="normal">nr</mml:mi>
                                          </mml:mrow>
