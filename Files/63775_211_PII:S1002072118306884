                                          <mml:mi>N</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo>ⅆ</mml:mo>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mo linebreak="badbreak">≈</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mi>σ</mml:mi>
                                          <mml:mo stretchy="true">¯</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>21</mml:mn>
                                       <mml:mi mathvariant="normal">a</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:msub>
                                    <mml:mi>N</mml:mi>
                                    <mml:mrow>
                                       <mml:mi mathvariant="normal">Tb</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mi mathvariant="normal">s</mml:mi>
                                 </mml:msub>
