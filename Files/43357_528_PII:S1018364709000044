         <head>
            <ce:article-footnote>
               <ce:note-para id="cenpara10" view="all">Peer-review under responsibility of King Saud University.</ce:note-para>
               <ce:note-para id="np320" view="all">
			               <ce:display>
                     <ce:figure id="f0320">
                        <ce:link locator="fx10"/>
                     </ce:figure>
